{
  "cSpell.words": [
    "Anantara",
    "autodocs",
    "autorestart",
    "avcbta",
    "birthdate",
    "bullmq",
    "clsx",
    "commitlint",
    "cyntler",
    "ELINK",
    "elkjs",
    "endregion",
    "fantasticon",
    "formulajs",
    "Grotesk",
    "hashids",
    "hookform",
    "iframe",
    "kbar",
    "lastname",
    "libphonenumber",
    "lumpsum",
    "luxon",
    "nestjs",
    "NEXTAUTH",
    "nextjs",
    "nocheck",
    "Omise",
    "oslllo",
    "overscan",
    "pageview",
    "papaparse",
    "payless",
    "persistor",
    "pmts",
    "popperjs",
    "ptps",
    "reactflow",
    "Redlock",
    "reduxjs",
    "resizer",
    "RETRYABLE",
    "Roboto",
    "Schengen",
    "signin",
    "SOMPO",
    "traveljoy",
    "typeorm",
    "usehooks",
    "UTCZ",
    "watchlist",
    "xsmall"
  ],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.fixAll.eslint"
  ],
  "search.exclude": {
    "node_modules": true,
    ".yarn": true,
    "yarn.lock": true,
    "package-lock.json": true,
    ".pnp.*": true,
    "**.svg": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "files.associations": {
    ".env.*": "env"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[env]": {
    "editor.defaultFormatter": "IronGeek.vscode-env"
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#fff",
    "titleBar.inactiveForeground": "#333",
    "titleBar.activeBackground": "#FF6B8B",
    "titleBar.inactiveBackground": "#b5862180",
    "sideBar.background": "#333",
    "sideBarSectionHeader.foreground": "#FF6B8B",
    "list.activeSelectionBackground": "#b5862180",
    "list.activeSelectionForeground": "#fff",
    "list.inactiveSelectionBackground": "#b5862180"
  },
  "jest.autoRun": {},
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "prettier.useEditorConfig": false,
  "prettier.configPath": ".prettierrc",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "tailwindCSS.experimental.classRegex": [
    ["(?:twMerge|tw|twJoin)\\(([^\\);]*)[\\);]", "[`'\"`]([^'\"`,;]*)[`'\"`]"]
  ]
}
